{
    "name": "cypresslab/php-curry",
    "description": "Curried functions in PHP",
    "keywords": ["curry", "functional-programming"],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {
        "psr-0": {"Cypress\\Curry": "src"},
        "files": ["src/Cypress/Curry/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "phpspec/prophecy-phpunit": "~1.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
